#include <iostream>
using namespace std;

void stripes(int x, int k){
    if (k % 2 == 1) {
        for (int i = 0; i < x; ++i) {
            cout << "=";
        }
    }
    else {
        for (int i = 0; i < x; ++i) {
            cout << "-";
        }
    }
}
void main()
{
    setlocale(LC_ALL, "Rus");
    int k = 0;
    for (int i = 0; i < 13; ++i) {
        ++k;
        if (i < 6) {
            for (int i = 0; i < 8; ++i) {
                cout << "* ";
            }
            stripes(30, k);
        }
        else {
            stripes(46, k);
        }
        cout << endl;
    }    
}
