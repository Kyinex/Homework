#include <iostream>

using namespace std;

int main() {
	for (float i = -10; i <= 10; i += 0.5) {
		int k = abs(sin(i)*10);
		if (sin(i) < 0) {
			k = 10 - k;
		}
		else {
			k = 10 + k;
		}

		string space = " ";
		int a = k / 2;
		for (a; a > 0; a--) {
			cout << " ";
		}
		cout << "                     .\n";
	}

	return 0;
}
