#include <iostream>
#include <Windows.h>
using namespace std;

int main()
{
    SetConsoleCP(1251);
    SetConsoleOutputCP(1251);
    for (double x = -4; x <= 4; x += 0.5)
    {
        if (x == 1)
        {
            cout << "Для x = 1 " << endl  << " решений нет" << endl;
        }
        else
        {
            cout << "Для x = " << x << endl << " y = " << (x*x-2*x+2)/(x-1) << endl;
        }
    }
}
