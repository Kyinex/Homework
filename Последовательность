#include <iostream>
#include <Windows.h>
using namespace std;

int main()
{
    SetConsoleCP(1251);
    SetConsoleOutputCP(1251);
    while (true) {
        double n;
        cout << "Введите натуральное число: ";
        void proverka(double& n); {
            while (!(cin >> n) || cin.get() != '\n')
            {
                cout << "Введите натуральное число: ";
                cin.clear();
                cin.ignore(32767, '\n');
            }
        }
        int n1 = (int)(n + 0.5);
        if (n1 <= 0) {
            n1 = 1;
        }
        for (int count = 0; count < 10; ++count) {
            cout << endl << "\t" << n1 + count;
        }
        cout << endl << endl;
    }
}
